/* pages/footmark/rank.wxss */

.rank-bg {
  height: 378rpx;
  width: 750rpx;
  background-position: bottom;
  background-image: url(data:image/png;base64,);
  position: relative;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center center;
}

.rank1 {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  top: 120rpx;
  left: 97rpx;
}

.rank2 {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  top: 28rpx;
  left: 295rpx;
}

.rank3 {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  top: 133rpx;
  left: 496rpx;
}

.rank1-user-icon {
  background-color: #fff;
  height: 92rpx;
  width: 92rpx;
  border: 4rpx solid #BCC3D0FF;
  border-radius: 50%;
}

.rank2-user-icon {
  background-color: #fff;
  height: 122rpx;
  width: 122rpx;
  border: 4rpx solid #FCC752FF;
  border-radius: 50%;
}

.rank3-user-icon {
  background-color: #fff;
  height: 92rpx;
  width: 92rpx;
  border: 4rpx solid #D3BC8AFF;
  border-radius: 50%;
}

.rank1-user-icon-tag {
  height: 34rpx;
  width: 34rpx;
  background-color: #BCC3D0FF;
  margin-top: -18rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
}

.rank2-user-icon-tag {
  height: 34rpx;
  width: 34rpx;
  background-color: #FCC752FF;
  margin-top: -18rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
}

.rank3-user-icon-tag {
  height: 34rpx;
  width: 34rpx;
  background-color: #D3BC8AFF;
  margin-top: -18rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
}

.user-icon-rank {
  color: #fff;
  font-size: 22rpx;
}

.nickname {
  margin-top: 4rpx;
  color: #fff;
  font-size: 24rpx;
  width: 132rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: center;
  line-height: normal;
}
.userPoint{
  color: #fff;
  font-size: 22rpx;
  width: 132rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: center;
  line-height: normal;
}
.honor-tag {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 136rpx;
  height: 38rpx;
  font-size: 22rpx;
  background-color: #4A82E2FF;
  border-radius: 19rpx;
  margin-top: 15rpx;
  padding: 0 12rpx;
}

.honor-text {
  color: #2D5EB2FF;
  font-size: 22rpx;
}

.ranklist {
  display: flex;
  flex-direction: column;
}

.ranklist-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 140rpx;
  border-bottom: 2rpx solid #F5F5F5FF;
}

.rank-num {
  padding-left: 50rpx;
  width: 96rpx;
  color: #666660FF;
  font-size: 32rpx;
}

.rank-logo {
  height: 72rpx;
  width: 72rpx;
  border-radius: 50%;
  background-color: lightblue;
  border: 4rpx solid #BCC3D0FF;
}

.rank-nickname {
  width: 380rpx;
  padding-left: 20rpx;
  color: #666660FF;
  font-size: 32rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.rank-userComment{
  font-size: 20rpx;
  display: inline-block;
  background:rgba(119,165,241,1);
  border-radius:4rpx;
  color:rgba(255,255,255,1);
  padding: 0 8rpx;
  margin-left: 8rpx;
}
.rank-score {
  color: #666660FF;
  font-size: 32rpx;
  width: 124rpx;
  text-align: right;
  padding-right:40rpx;
}

.group-btn {
  display: flex;
  width: 100%;
  align-items: center;
  justify-content: center;
  padding-left: 0;
  padding-right: 0;
  width: 252rpx;
  height: 84rpx;
  margin-top: 60rpx;
  border-radius: 8rpx;
  border: 2rpx solid #7BA8F2FF;
}

.grouprank {
  width: 252rpx;
  height: 80rpx;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
  cursor: pointer;
}

.grouprank-text {
  color: #7BA8F2FF;
  font-size: 32rpx;
}