.container {
    width: 100%;
    font-size: 34rpx;
}
.facility_container {
    width: 100%;
    min-height: 100%;
    box-sizing: border-box;
    position: relative;
    background: #F6F6F6;
    padding: 341rpx 30rpx 30rpx 30rpx;
    z-index: 1;
}
.bg{
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}
.facility_container .content{
    width: 100%;
    background: #FFF;
    box-sizing: border-box;
    padding: 37rpx 30rpx 77rpx 30rpx;
    border-radius: 20rpx;
}
.facility_container h1{
    color: #3B3B3B;
    font-size: 30rpx;
    line-height: 42rpx;
    width: 100%;
    display: block;
    text-align: center;
}
.facility_container .desc{
    margin-top: 71rpx;
    color: #969696;
    font-size: 28rpx;
    line-height: 60rpx;
    box-sizing: border-box;
}

.master_container{
    width: 100%;
    box-sizing: border-box;
    padding: 192rpx 30rpx 30rpx 30rpx;
    background: #F6F6F6;
    position: relative;
    z-index: -1;
    min-height: 100%;
}

.master_container .bg {
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}
.master_container .head {
    width: 100%;
    height: 395rpx;
    color: #969696;
    font-size: 28rpx;
    line-height: 60rpx;
    background: #FFF;
    border-radius: 20rpx;
    box-sizing: border-box;
    padding-top: 66rpx;
    padding-left: 53rpx;
    padding-bottom: 41rpx;
}
.master_container .head h1 {
    color: #3B3B3B;
    font-size: 40rpx;
    line-height: 56rpx;
}
.master_container .desc {
    background: #FFF;
    margin-top: 30rpx;
    border-radius: 20rpx;
    padding: 20rpx;
    font-size: 28rpx;
    color: #969696
}

/*解析样式*/
.wxParse-p {
    box-sizing: border-box;
    margin: 0 !important;
}
.wxParse-img {
    width: 100%;
}
.video {
    width: 100%;
    margin-top: 30rpx;
    position: relative;
    border-radius: 30rpx;
}