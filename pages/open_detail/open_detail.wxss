.openDetail{
  padding-bottom: 30rpx;
  position: relative;
}
.banner{
  width: 100%;
  height: 400rpx;
}
.banner .img{
  width: 100%;
  height: 100%;
}
.container{
  position: relative;
  top: -30rpx;
  border-radius: 24rpx 24rpx 0 0;
  background: #fff;
  padding-top: 54rpx;
}
.title .name{
  font-size:34rpx;
  font-weight:500;
  color:rgba(120,201,139,1);
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-left: 38rpx;
}
.orderNow{
  width: 100%;
  height: 120rpx;
  position: fixed;
  bottom: 0;
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.bottomline{
  width: 100%;
  height: 120rpx;
  background:rgba(239,239,244,1);
}
.toOrder{
  width:710rpx;
  height:94rpx;
  background:rgba(120,201,139,1);
  border-radius:20rpx;
  color: #fff;
  line-height: 94rpx;
  text-align: center;
  font-size: 36rpx;
}
.disabled{
  background:#969696;
}
.info{
  width: 90%;
  margin: 30rpx auto 0;
}
.info .desc{
  color: #969696;
  font-size: 28rpx;
  line-height: 60rpx;
  box-sizing: border-box;
}
.desc image {
  margin: 0 auto;
}
.viewSteps{
  width: 90%;
  margin: 30rpx auto;
}
.stepTitle{
  font-size: 34rpx;
  font-weight: 600;
  margin-bottom: 26rpx;
  display: flex;
  align-items: center;
}
.line{
  width:8rpx;
  height:32rpx;
  background:rgba(120,201,139,1);
  border-radius:4rpx;
  margin-right: 12rpx;
}
.swiper_item{
  border-radius: 30rpx;
}
.imageBox{
  width: 95%;
  height: 100%;
  border-radius: 30rpx;
  position: relative;
}
.imageNameMask{
  position: absolute;
  bottom: 0;
  height:68rpx;
  width: 100%;
  background:rgba(0,0,0,0.5);
  border-radius: 0 0 30rpx 30rpx;
}
.imageBox .imageName{
  display: block;
  width: 80%;
  line-height: 68rpx;
  margin: 0 auto;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #fff;
  font-size: 28rpx;
}
.imageBox .image{
  border-radius: 30rpx;
  width: 100%;
  height: 100%;
}
.address{
  width: 90%;
  margin: 44rpx auto 0;
}
.locImg{
  width: 28rpx;
  height: 32rpx;
}
.address .title{
  font-size: 28rpx;
  font-weight: 600;
  width: 100%;
}
.address .info{
  font-size: 24rpx;
  width: 100%;
  margin: 0;
  display: flex;
  align-items: center;
}
.location_desc{
  width:600rpx;
  font-size:26rpx;
  font-weight:400;
  color:rgba(178,178,178,1);
  margin-left: 4rpx;
  margin-top: 4rpx;
}
.map{
  margin-top: 30rpx;
  border-radius:16rpx;
  overflow: hidden;
}
.rules{
  width: 90%;
  margin: 30rpx auto 0;
  font-size: 26rpx;
}
.rules .title{
  font-size: 28rpx;
  font-weight: 600;
  width: 100%;
}
.rulesMain{
  font-size: 30rpx;
  color: #353535;
}
.visitTime{
  color: #888888;
}
.phoneImg{
  width: 24rpx;
  height: 32rpx;
  margin-right: 12rpx;
}
.phone{
  display: flex;
  align-items: center;
  color: #666666;
}
.phonecall{
  color: #78C98B;
}