/* 选中时的样式 */
.active0, .active1, .active2, .active3 {
    line-height: 100rpx;
    text-align: center;
    font-size: 30rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #73DC4E;
    flex: 1;
    text-align: center;
    width:max-content
}


/* 下划线的样式 */
.nav_underline0, .nav_underline1, .nav_underline2, .nav_underline3 {
    background: #73DC4E;
    width: 120rpx;
    height: 6rpx;
    margin-top: -10rpx;
    border-radius: 8rpx;
    margin-left: calc(50% - 60rpx);
}

.nav_underline1 {
    margin-left: calc(50% - 60rpx);
}

.nav_underline2 {
    margin-left: calc(50% - 60rpx);
}

