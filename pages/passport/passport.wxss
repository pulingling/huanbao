}

/* video */

.box-w {
    width: 100%;
    height: 311rpx;
    border-radius: 20rpx;
}

.videocover {
    position: absolute;
    top: 0;
    width: 100%;
    overflow: hidden;
    border-radius: 20rpx;
    z-index: 3;
}

.videocoverbg {
    width: 100%;
    height: 73rpx;
    font-size: 30rpx;
    background: rgba(0, 0, 0, 0.3);
    border-radius: 20rpx 20rpx 0rpx 0rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    line-height: 73rpx;
    color: #FFFFFF;
}

.playbtn {
    position: absolute;
    top: 50%;
    z-index: 2;
    left: 50%;
    width: 64rpx;
    height: 84rpx;
    transform: translate(-50%, -50%);
}

.videocover .cover {
    width: 100%;
}

.ico-share {
    width: 18px;
    height: 18px;
    display: block;
}

.nav_tab {
    position: relative;
    top: -200rpx;
    width: 702rpx;
    margin: 20rpx auto;
    height: 100rpx;
    display: flex;
    background: #F7F7F7;
    border-radius: 10rpx;
