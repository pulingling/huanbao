.certificates{
  display: flex;
  flex-direction: column;
  align-items:center;
  width: 100%;
  height: 100%;
  margin-top:-200rpx;
}

.certificates>.selectors-set,.certificates>.selectors-set>view{
  display: flex;
  align-items: center;
  font-size:24rpx;
}
.certificates>.selectors-set{
  width:100%;
  flex-direction: column;
  align-items: flex-start;
  padding-left:20rpx;
  box-sizing: border-box;
}
.certificates>.selectors-set>view{
  margin:12rpx 0 12rpx 18rpx;
}
.certificates>.selectors-set>view>.label{
  color:#333;
  font-weight:600;
  font-size:32rpx;
}
.certificates>.selectors-set>view>picker{
  background-color:#fff;
  border:1rpx solid #409852;
  padding:3rpx 12rpx;
  margin-left:12rpx;
  width:max-content;
  border-radius: 8rpx;
  box-shadow: inset 0 0 10rpx #eee8;
  color:#333;
  font-size:28rpx;

}
.certificates>.selectors-set>view>picker .picker{
  display: flex;
  align-items: flex-start;
  flex-direction: row;
}
.certificates>.selectors-set>view>picker .down-arrow{
  width:12rpx;
  height:12rpx;
  transform-origin: 50% 50%;
  transform:rotateZ(45deg);
  border-right:4rpx solid #333;
  border-bottom: 4rpx solid #333;
  margin-right:10rpx;
  margin-left:18rpx;
  margin-top:12rpx;
}
.certList{
  width:100%;
  min-height:1200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.certList>.cert-item{
  width:702rpx;
  height:130rpx;
  background-color:#fff;
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  margin-top:24rpx;
}
.cert-item>.pdf-logo{
  width:130rpx;
  height:152rpx;
  flex:0 0 auto;
  margin-left:28rpx;
}
.cert-item>.mid{
  display: flex;
  flex-direction: column;
  flex:0 0 auto;
  align-items:flex-start;
  margin-left:34rpx;
}
.cert-item>.mid>.individual-name{
  font-size:28rpx;
  color:#333;
  font-weight:600;
  max-width:400rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.cert-item>.mid>.school-name{
  padding:6rpx 12rpx;
  background-color:#F1FAEF;
  border-radius: 12rpx;
  color:#515A68;
  font-size:24rpx;
  max-width:260rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.cert-item>.mid>.year{
  color:#93A1B6;
  font-size:24rpx;
}
.cert-item>.right{
  display: flex;
  flex-direction: column;
  align-self: flex-start;
  color:#6AA814;
  font-size:24rpx;
  margin-right:30rpx;
  margin-left:auto;
}
.cert-item>.right>.top{
  display: flex;
  align-self: center;
  width:max-content;
  height:auto;
  background-color: transparent;
  background-image:none;
}
.cert-item>.right>.top>.tag{
  width:36rpx;
  height:52rpx;
  margin-left:8rpx;
  flex:0 0 auto;
  background-color:#6AA814;
  clip-path:polygon(0% 0%,100% 0,100% 100%,50% 80%,0% 100%, 0% 0%);
}

.view-background {
  width: 100%;
  height: 178px; /* 根据需要调整高度 */
  position: relative;
  margin-bottom: 80rpx;
  text-align: center;
}

.background-image {
  width: 100%;
  height: 100%;
}

.button-container {
  position: absolute;
  right: 17px; /* 右边距 */
  bottom: 10px; /* 底部边距 */
  display: flex;
  align-items: center; /* 垂直居中对齐按钮 */
}

.update-button1 {
  height: 26px; /* 设置按钮高度 */
  margin-right: 10px; /* 按钮之间的间距 */
  display: flex;
  align-items: center; /* 垂直居中对齐文字 */
  justify-content: center; /* 水平居中对齐文字 */
  text-align: center; /* 确保文字在按钮中居中 */
  border-radius: 20px; /* 设置按钮的圆角半径为10px */
  background-color: #2A9508;
  color: rgb(250, 250, 250);
  font-size: 30rpx;
  font-weight: bold;
  bottom: 35px; /* 按钮距离底部的距离 */
  z-index: 1;   /* 确保按钮在图片之上 */
}

.update-button2 {
  height: 26px; /* 设置按钮高度 */
  margin-right: 10px; /* 按钮之间的间距 */
  display: flex;
  align-items: center; /* 垂直居中对齐文字 */
  justify-content: center; /* 水平居中对齐文字 */
  text-align: center; /* 确保文字在按钮中居中 */
  border-radius: 20px; /* 设置按钮的圆角半径为10px */
  background-color: #81C97B;
  color: rgb(250, 250, 250);
  font-size: 30rpx;
  font-weight: bold;
  bottom: 35px; /* 按钮距离底部的距离 */
  z-index: 1;   /* 确保按钮在图片之上 */
}

.icon-image {
  width: 16px; /* 根据实际图标大小调整 */
  height: 16px;
  margin-right: 5px; /* 图标和文字之间的间隔 */
}