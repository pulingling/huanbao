  border-radius: 24rpx 24rpx 0 0;
}
.addBtn{
  width:710rpx;
  height:94rpx;
  background:rgba(120,201,139,1);
  border-radius:20rpx;
  line-height: 94rpx;
  text-align: center;
  font-size:36rpx;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.addperson .weui-dialog__ft{
  padding: 0;
