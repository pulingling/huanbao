Page{
  background:rgba(239,239,244,1);
  padding-bottom: 90rpx;
}
.orderDetail{
  background:rgba(239,239,244,1);
}
.tab_container {
  color: #969696;
  background: #FFF;
  font-size: 28rpx;
  line-height: 40rpx;
  height: 100rpx;
  margin-top: 0;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  box-sizing: border-box;
  padding: 26rpx 108rpx 16rpx 108rpx;
}
.tab_container text {
  vertical-align: bottom;
  transition: all .2s ease-out;
  display: block;
  width: 50%;
  text-align: center;
}
.tab_container .active {
  color: #6ECE83;
  font-size: 36rpx;
  line-height: 50rpx;
  position: relative;
}
.tab_container .active::after {
  content: " ";
  display: block;
  background: #6ECE83;
  height: 4rpx;
  width: 46rpx;
  position: absolute;
  left: 0;
  right: 0;
  margin: 0 auto;
}
.addjoiner{
  width:710rpx;
  height:168rpx;
  background:rgba(255,255,255,1);
  border-radius:12rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 40rpx;
  margin: 20rpx auto 0;
}
.addLeft{
  display: flex;
  align-items: center;
}
.addLeft .imgBox{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 40rpx;
}
.imgBox .img{
  width: 80rpx;
  height: 80rpx;
}
.addText{
  font-size:34rpx;
  font-weight:400;
  color:rgba(0,0,0,0.9);
}
.addRight{
  width: 40rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  border: 1px solid #fff;
  font-size: 28rpx;
  border-radius: 50%;
}
.timeRange{
  display: block;
  width: 100%;
  font-size:28rpx;
  font-weight:400;
  color:rgba(53,53,53,1);
  text-align: center;
}
.calendarBtnBox{
  display: flex;
  width: 100%;
  border-top: 2rpx solid #eee;
  margin-top: 40rpx;
}
.calendarBtn{
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 120rpx;
  font-weight: 600;
}
.confirm{
  border-left: 1px solid #eee;
  color: #576B95;
}
.chooseTimeRange{
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.timeChoose{
  display: flex;
  flex-wrap: wrap;
  margin-top: 24rpx;
  justify-content: space-around;
  background: rgba(245,245,245,1);
  border-radius: 8rpx;
  font-size: 28rpx;
  color: rgba(155,155,155,1);
  padding: 15rpx;
}
.choosed{
  background: rgba(120,201,139,1);
  color: #FFFFFF;
}
.timeChooseItem{
  width:212rpx;
  height:76rpx;
  background:rgba(245,245,245,1);
  border-radius:8rpx;
  line-height: 76rpx;
  text-align: center;
  font-size: 28rpx;
  color:rgba(155,155,155,1);
}
.timeActive{
  background:rgba(120,201,139,1);
  color: #FFFFFF;
}
.orderInfo{
  width: 100%;
  margin-top: 20rpx;
}
.infoItem{
  display: flex;
  height: 112rpx;
  align-items: center;
  background:rgba(255,255,255,1);
  width: 100%;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 34rpx;
}
.infoItem .itemName{
  word-break: keep-all;
  display: block;
  margin-right: 40rpx;
  width: 220rpx;
}
.slider{
  width: 350rpx;
}
.arrowRight:after{
  content: '';
    display: inline-block;
    width: 8px;
    height: 8px;
    border-top: 1px solid #656565;
    border-right: 1px solid #656565;
    transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
}
.visitTime{
  margin-top: 20rpx;
}
.chooseDate{
  color: #969696;
}
.joinnerNum{
  width: 100%;
  height: 55rpx;
  background: rgb(182, 222, 242);
  margin: 30rpx auto;
  line-height: 55rpx;
  text-align: center;
  color: #fff;
  position: relative;
}
.closeJoinner{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 20rpx;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  border: 1px solid #fff;
  line-height: 30rpx;
  text-align: center;
  font-size: 24rpx;
}
.noticeinfo{
  width: 90%;
  margin: 30rpx auto 0;
  font-size: 28rpx;
  color:#969696;
}
.noticeTitle{
  color: #78C98B;
}
.submit{
  width: 100%;
  height:180rpx;
  background:rgba(255,255,255,1);
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 1;
}
.submitBtn{
  width: 90%;
  height: 90rpx;
  background:rgba(120,201,139,1);
  color: #fff;
  border-radius: 15rpx;
  line-height: 90rpx;
  text-align: center;
  margin:0 0 60rpx;
}
.group .infoItem{
  height: 100rpx;
}
.calendarDialog .dialog--weui-dialog__bd{
  padding: 0 ;
}
.calendarDialog{
  width: 100vw;
  top: 0;
